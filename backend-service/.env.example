# Server Configuration
app_port=9110

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ORIGINS=http://localhost,http://localhost:3000,http://localhost:5173

# Feature Flags
# Enable mock data for all users (set to 'true' to enable, 'false' or omit to disable)
# Default: false (mock data disabled)
ENABLE_MOCK_DATA=false

# Test user ID - user with this ID will have access to mock data even when ENABLE_MOCK_DATA is false
# This allows demo/test users to see mock transactions while regular users see real data only
# Example: TEST_USER_ID=999
TEST_USER_ID=
